int ledPin = 13; // You can adjust this as needed

void setup() {
  Serial.begin(9600);
  pinMode(ledPin, OUTPUT);
}

void loop() {
  if (Serial.available() > 0) {
    int note = Serial.parseInt();
    // Map the note value to the LED brightness or color control
    int ledValue = map(note, 0, 127, 0, 255);
    analogWrite(ledPin, ledValue);
  }
}
